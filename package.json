{
	"name": "portfolio",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build --turbopack",
		"start": "next start",
		"lint": "pnpm exec eslint .",
		"lint:fix": "pnpm lint --fix",
		"lint:changed": "eslint --cache",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"pre-commit": "pnpm type-check && pnpm lint",
		"pre-commit:fast": "pnpm lint:changed",
		"format:lint": "pnpm format && pnpm lint:fix",
		"validate": "pnpm format:check && pnpm pre-commit && pnpm build",
		"setup-hooks": "cp scripts/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit || icacls .git\\hooks\\pre-commit /grant Everyone:F",
		"postinstall": "pnpm setup-hooks"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.7",
		"clsx": "^2.1.1",
		"critters": "^0.0.25",
		"embla-carousel": "^8.6.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-react": "^8.6.0",
		"framer-motion": "^12.23.12",
		"howler": "^2.2.4",
		"next": "15.5.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-tooltip": "^5.29.1",
		"react-type-animation": "^3.2.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@next/eslint-plugin-next": "^15.5.0",
		"@svgr/webpack": "^8.1.0",
		"@tailwindcss/postcss": "^4.1.12",
		"@types/howler": "^2.2.12",
		"@types/node": "^20.17.50",
		"@types/react": "19.1.5",
		"@types/react-dom": "19.1.5",
		"@types/webpack": "^5.28.5",
		"eslint": "^9.33.0",
		"eslint-config-next": "15.4.6",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"prettier": "^3.6.2",
		"svgo-loader": "^4.0.0",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"webpack": "^5.101.3"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "19.1.5",
			"@types/react-dom": "19.1.5"
		},
		"onlyBuiltDependencies": [
			"sharp",
			"unrs-resolver"
		]
	}
}
