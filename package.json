{
	"name": "portfolio",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build --turbopack",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "pnpm lint --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"pre-commit": "pnpm type-check && pnpm lint",
		"format:lint": "pnpm format && pnpm lint:fix",
		"validate": "pnpm format:check && pnpm pre-commit && pnpm build",
		"setup-hooks": "cp scripts/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit || icacls .git\\hooks\\pre-commit /grant Everyone:F",
		"postinstall": "pnpm setup-hooks"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.4",
		"@radix-ui/react-slot": "^1.2.3",
		"@types/howler": "^2.2.12",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"critters": "^0.0.25",
		"embla-carousel": "^8.6.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-react": "^8.6.0",
		"framer-motion": "^12.12.2",
		"howler": "^2.2.4",
		"lucide-react": "^0.477.0",
		"next": "15.4.0-canary.50",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-tooltip": "^5.28.1",
		"react-type-animation": "^3.2.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@next/eslint-plugin-next": "^15.4.5",
		"@tailwindcss/postcss": "^4.1.7",
		"@types/node": "^20.17.50",
		"@types/react": "19.1.5",
		"@types/react-dom": "19.1.5",
		"@types/webpack": "^5.28.5",
		"eslint": "^9.27.0",
		"eslint-config-next": "15.4.0-canary.50",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.3",
		"eslint-plugin-react-hooks": "^5.2.0",
		"prettier": "^3.6.2",
		"tailwindcss": "^4.1.7",
		"typescript": "^5.8.3",
		"webpack": "^5.99.9"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "19.1.5",
			"@types/react-dom": "19.1.5"
		}
	}
}
